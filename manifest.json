{

  "manifest_version": 2,
  "name": "Remober",
  "version": "1.0",

  "description": "Replaces the B emoji with a standard ASCII character.",

  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["remober.js"]
    }
  ]

}
